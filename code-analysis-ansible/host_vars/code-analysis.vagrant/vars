---

# Compute Engine
compute_engine:
  initial_memory: 128m
  max_memory: 200m

# Elastic Search
elastic_search:
  initial_memory: 200m
  max_memory: 200m

# Login
ansible_ssh_private_key_file: "{{ key_path }}/vagrant"
